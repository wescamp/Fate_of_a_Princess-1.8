#: Fate_of_a_Princess/scenarios/02_Escaping_from_Wesnoth.cfg:167
msgid "Treasure Caravan"
msgstr "Kincsesszekér"

#. [unit]: type=Foap Caravan, id=Caravan 2
#: Fate_of_a_Princess/scenarios/02_Escaping_from_Wesnoth.cfg:179
msgid "Supply Caravan"
msgstr "Társzekér"

